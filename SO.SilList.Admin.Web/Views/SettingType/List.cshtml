@model IEnumerable<SO.SilList.Manager.Models.ValueObjects.SettingTypeVo>

<p>
   <a href="@Url.Action("Create")" class="btn btn-info">Create New</a>

</p>

<table class="table table-bordered table-hover">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.siteId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.groupName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.value)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.environmentTypeId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.created)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.isActive)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.siteId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.groupName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.value)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.environmentTypeId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.created)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.isActive)
        </td>
        <td style="width:200px">
            <a href="@Url.Action("Edit",new { id=item.settingTypeId })" class="btn">Edit</a>
            <a href="@Url.Action("Details",new { id=item.settingTypeId })" class="btn">Details</a>
            <a href="@Url.Action("Delete",new { id=item.settingTypeId })" class="btn">Delete</a>
            @*@Html.ActionLink("Delete", "Delete", new { id=item.settingTypeId })*@

        </td>
    </tr>
}

</table>
