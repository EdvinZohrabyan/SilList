@model SO.SilList.Manager.Models.ViewModels.ModelTypeVm
@{
    ViewBag.Title = "Models";
    Layout = "~/Views/Shared/_InnerLayout.cshtml";
}

<h2>@ViewBag.Title</h2>

<form id="filterForm" class="form-inline" method="get" action="@Url.Action("Index","ModelType")">
    
    @Html.Action("Filter",@Model)
    <div class="pull-right">
        @Model.paging.rowCount of @Model.result.Count
    </div>
    @Html.Partial("_List", @Model.result)
    @Html.Action("Pagination", @Model.paging)
</form>
