@model IEnumerable<SO.SilList.Manager.Models.ValueObjects.BusinessImagesVo>

<table class="table table-bordered table-hover">
    <tr>
        <th>Image 
        </th>
        <th>Business
        </th>
        <th>
            @Html.DisplayNameFor(model => model.created)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.isActive)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.image.name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.business.name)
            </td>
            <td>
               <span>@String.Format("{0:MM/dd/yyy}", item.created)</span>
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.isActive)
            </td>
            <td>
                <a href="@Url.Action("Edit", new { id = item.businessImageId })" class="btn">Edit</a>
                <a href="@Url.Action("Details", new { id = item.businessImageId })" class="btn">Details</a>
                <a href="@Url.Action("Delete", new { id = item.businessImageId })" class="btn">Delete</a>
            </td>
        </tr>
    }

</table>
