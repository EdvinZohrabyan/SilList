@{
    if (ViewContext.Controller.ViewBag.Images != null)
    {
        foreach (SO.SilList.Manager.Models.ValueObjects.ImageVo img in ViewContext.Controller.ViewBag.Images)
        {
            <hr />
            <img src="@img.url" width="200px" /> 
        }
    }
    else
    {
            <hr />
        <strong>Cand access ViewBag from partial view</strong>
    }
}