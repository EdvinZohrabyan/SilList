@model SO.SilList.Manager.Models.ViewModels.CarVm
@{
    ViewBag.Title = "Cars";
    Layout = "~/Views/Shared/_InnerLayout.cshtml";
    }

<h2>@ViewBag.Title</h2>

<form id="filterForm" class="form-inline" method="get" action="@Url.Action("Index","Car")">
    
    @Html.Action("Filter",@Model)
    <div class="pull-right">
        @Model.result.Count of @Model.rowCount 
    </div>
    @Html.Partial("_List", @Model.result)
    @Html.Action("Pagination", @Model)
</form>
