@model SO.SilList.Manager.Models.ViewModels.ListingVm
@{
    ViewBag.Title = "Listings";
    Layout = "~/Views/Shared/_InnerLayout.cshtml";
}

<h2>@ViewBag.Title</h2>

<form name="filterForm" class="form-inline" method="get" action="@Url.Action("Index","Listing")">

 
@Html.Action("Filter", @Model)

<p style="margin-top:10px; margin-bottom:10px;"> </p>

<div class="pull-right">
  Showing @Model.firstVisibleRow - @Model.lastVisibleRow of @Model.totalPages
  @*  @Model.resultPerPage of @Model.totalPages *@


</div>

@Html.Partial("List", @Model.result)

@Html.Action("Pagination", @Model)

@* @Html.ActionLink("Clear Stats", "Delete") *@

 </form>