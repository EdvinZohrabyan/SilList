@model IEnumerable<SO.SilList.Manager.Models.ValueObjects.BusinessVo>

<p>
   <a href="@Url.Action("Create")" class="btn btn-info">Create New</a>

</p>
<table class="table table-bordered table-hover">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.siteId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>
        
        <th>
            @Html.DisplayNameFor(model => model.city)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.state)
        </th>
     
       
       
        <th>
            @Html.DisplayNameFor(model => model.created)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.isActive)
        </th>
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.siteId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
      
        <td>
            @Html.DisplayFor(modelItem => item.city)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.state)
        </td>
       

        <td>
            <span>@String.Format("{0:MM/dd/yyy}", item.created)</span>
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.isActive)
        </td>
      
        <td>

            <a href="@Url.Action("Edit",new { id=item.businessId })" class="btn">Edit</a>
            <a href="@Url.Action("Details",new { id=item.businessId })" class="btn">Details</a>
            <a href="@Url.Action("Delete",new { id=item.businessId })" class="btn">Delete</a>

          
        </td>
    </tr>
}

</table>
