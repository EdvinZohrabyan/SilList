@model IEnumerable<SO.SilList.Manager.Models.ValueObjects.ListingImagesVo>

<p>
    @Html.ActionLink("Create New", "Create")
</p>

<table class="table table-bordered table-hover">
    <tr>     
        <th>
            @Html.DisplayNameFor(model => model.created)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.isActive)
        </th>
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.isActive)
        </td>
      
        <td>

            <a href="@Url.Action("Edit",new { id=item.listingImageId })" class="btn">Edit</a>
            <a href="@Url.Action("Details",new { id=item.listingImageId })" class="btn">Details</a>
            <a href="@Url.Action("Delete",new { id=item.listingImageId })" class="btn">Delete</a>

          
        </td>
    </tr>
}

</table>
